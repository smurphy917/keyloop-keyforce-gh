<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Single_Price_Type</fullName>
    <active>true</active>
    <errorConditionFormula>AND(PriceTierCount__c &gt; 0, NOT(ISNULL(Price__c)))</errorConditionFormula>
    <errorMessage>A Price List Line cannot have both Price Tiers and a Price value defined.</errorMessage>
</ValidationRule>
